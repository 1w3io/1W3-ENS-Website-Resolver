{
  "manifest_version": 3,
  "name": "1W3 - DWeb Gateway",
  "version": "1.0",
  "permissions": [
    "webNavigation",
    "storage"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
